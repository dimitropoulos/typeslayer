{
	"$schema": "https://schema.tauri.app/config/2",
	"productName": "typeslayer",
	"version": "0.0.0",
	"identifier": "typeslayer",
	"build": {
		"beforeDevCommand": "pnpm dev",
		"devUrl": "http://localhost:1993",
		"beforeBuildCommand": "pnpm build",
		"frontendDist": "../dist"
	},
	"app": {
		"windows": [
			{
				"title": "TypeSlayer",
				"width": 1920,
				"height": 1080,
				"zoomHotkeysEnabled": true
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/64x64.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	}
}
