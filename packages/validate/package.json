{
  "name": "@typeslayer/validate",
  "version": "0.1.26",
  "description": "Validation schemas and utilities for TypeScript compiler trace analysis",
  "license": "MIT",
  "private": false,
  "type": "module",
  "repository": "github:dimitropoulos/typeslayer",
  "homepage": "https://github.com/dimitropoulos/typeslayer#readme",
  "bugs": "https://github.com/dimitropoulos/typeslayer/issues",
  "keywords": [
    "typescript",
    "performance",
    "analysis",
    "validation",
    "trace"
  ],
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@10.6.5",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./node": {
      "types": "./dist/node.d.mts",
      "import": "./dist/node.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit",
    "attw": "attw --pack --profile esm-only .",
    "test": "vitest"
  },
  "dependencies": {
    "@mui/icons-material": "7.3.6",
    "@mui/material": "7.3.6",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@types/node": "^24.10.1",
    "@types/stream-json": "^1.7.8",
    "tsdown": "^0.17.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  }
}
