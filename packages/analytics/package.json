{
  "name": "@typeslayer/analytics",
  "version": "0.1.26",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.6.5",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "scripts": {
    "dev": "pnpm wrangler dev --local",
    "build": "tsdown",
    "predeploy": "pnpm build",
    "deploy": "pnpm wrangler deploy",
    "migrate:apply": "pnpm wrangler d1 migrations apply typeslayer --remote",
    "migrate:local": "pnpm wrangler d1 migrations apply typeslayer --local",
    "migrate:gen": "pnpm wrangler d1 migrations create typeslayer",
    "db:exec": "pnpm wrangler d1 execute typeslayer",
    "push:local": "node scripts/push-events.ts --local",
    "push:remote": "node scripts/push-events.ts --remote"
  },
  "dependencies": {
    "@typeslayer/validate": "workspace:*",
    "@typeslayer/rust-types": "workspace:*"
  },
  "devDependencies": {
    "wrangler": "^4.54.0",
    "tsdown": "^0.17.2",
    "@types/node": "^25.0.3",
    "@cloudflare/workers-types": "^4.20251225.0"
  }
}
