{
  "name": "@typeslayer/analyze-trace",
  "version": "0.1.17",
  "description": "Analyze TypeScript compiler trace events to identify performance bottlenecks",
  "license": "MIT",
  "private": false,
  "type": "module",
  "repository": "github:dimitropoulos/typeslayer",
  "homepage": "https://github.com/dimitropoulos/typeslayer#readme",
  "bugs": "https://github.com/dimitropoulos/typeslayer/issues",
  "keywords": [
    "typescript",
    "performance",
    "analysis",
    "trace",
    "cli"
  ],
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@10.6.5",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./browser": {
      "types": "./dist/browser.d.mts",
      "import": "./dist/browser.mjs"
    }
  },
  "bin": {
    "typeslayer-analyze-trace": "./bin/typeslayer-analyze-trace.mjs"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "attw": "attw --pack --profile esm-only .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@typeslayer/validate": "workspace:*",
    "zod": "4.1.13"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@types/node": "24.10.1",
    "tsdown": "^0.17.2",
    "typescript": "5.9.3"
  }
}
