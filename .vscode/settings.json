  "cSpell.words": [
    "archlinux",
    "Autorenew",
    "bivariant",
    "checktypes",
    "chrono",
    "cpuprofile",
    "Dimitri",
    "Freshable",
    "gibibyte",
    "hotspot",
    "Hotspots",
    "indexmap",
    "Kabaddi",
    "kibibyte",
    "Lapce",
    "layercake",
    "majenta",
    "mebibyte",
    "mibibyte",
    "Millis",
    "Mitropoulos",
    "nvim",
    "perfetto",
    "Simplifiable",
    "Sonoma",
    "speedscope",
    "subl",
    "tauri",
